@model GodRun_API_el_bueno_.ViewModels.PartidaViewModel;
@{
    ViewData["Title"] = "HallOfFame";
}

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.min.css" />

</head>
<div class="container">

    <h1>Hall Of Fame</h1>
    <script>


    </script>

    <table class="table">
        <thead class="thead-dark">
            <tr>

                <th scope="col">#</th>
                <th scope="col">Points</th>
                <th scope="col">Kills</th>
                <th scope="col">Time</th>
                <th scope="col">Date</th>
                <th scope="col">Name</th>
            </tr>
        </thead>

        <tbody>

            @{ int cnt = 1;}
            @foreach (var item in Model.partidak)
            {

            <tr>

                <td>
                    @cnt
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.puntuazioa)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.kills)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.time)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.employee.name)
                </td>
                @{cnt++;}
            </tr>
            }


        </tbody>
    </table>



    <a id="linkdercarga" asp-controller="Report" asp-action="PrintHoF">DESCARGAR PDF</a>
