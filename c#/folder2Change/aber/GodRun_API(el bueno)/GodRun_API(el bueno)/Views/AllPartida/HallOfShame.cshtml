                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.kills)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.time)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.date)
                    </td>

                </tr>
            }
        <a asp-controller="Report" asp-action="PrintHoS">DESCARGAR PDF</a>
        </tbody>
    </table>

